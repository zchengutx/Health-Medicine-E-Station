{
  "name": "uni_app",
  "version": "1.0.0",
  "description": "Health Medicine E-Station Uni-App",
  "main": "main.js",
  "scripts": {
    "dev:h5": "uni build --watch --platform h5",
    "dev:mp-weixin": "uni build --watch --platform mp-weixin",
    "build:h5": "uni build --platform h5",
    "build:mp-weixin": "uni build --platform mp-weixin",
    "serve": "http-server dist/build/h5 -p 8080",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": ["uni-app", "vue", "health", "medicine"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src"],
    "testMatch": ["**/__tests__/**/*.test.ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
